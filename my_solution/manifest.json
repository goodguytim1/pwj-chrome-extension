{
    "name": "Sunshine Activity Planner",
    "version": "1.0",
    "description": "Sunshine chrome extension to track actions!",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/sun.png",
        "32": "images/icons321.png",
        "48": "images/icons64.png",
        "128": "images/icons1281.png"
      }
    },
    "permissions": [
      "storage",
      "tabs",
      "contextMenus"
    ],
    "background": {
      "scripts": ["./packages/uuidv4.min.js", "./js/background.js"],
      "persistent": false
    },
    "icons": {
      "16": "images/sun.png",
      "32": "images/icons321.png",
      "48": "images/icons64.png",
      "128": "images/icons1281.png"
    },
    "manifest_version": 2
  }